{
  "name": "deku-redux",
  "version": "1.1.0",
  "description": "Redux bindings for deku",
  "main": "dist/commonjs/index.js",
  "jsnext:main": "modules/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "_mocha",
    "build:cjs": "babel modules --out-dir dist/commonjs",
    "build:umd": "rollup -c",
    "build": "npm run clean && npm run build:cjs && npm run build:umd",
    "clog": "conventional-changelog -p angular -i CHANGELOG.md -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/troch/deku-redux.git"
  },
  "keywords": [
    "deku",
    "redux"
  ],
  "author": "Thomas Roch <thomas.c.roch@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/troch/deku-redux/issues"
  },
  "homepage": "https://github.com/troch/deku-redux#readme",
  "dependencies": {
    "invariant": "^2.1.2",
    "is-equal-shallow": "^0.1.3",
    "is-plain-object": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-plugin-transform-export-extensions": "^6.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.1.1",
    "component-mock": "0.0.2",
    "conventional-changelog": "^0.5.3",
    "mocha": "^2.3.3",
    "rimraf": "^2.5.0",
    "rollup": "^0.24.1",
    "rollup-plugin-babel": "^2.3.8",
    "rollup-plugin-commonjs": "^2.1.0",
    "rollup-plugin-npm": "^1.2.1",
    "should": "^7.1.1"
  },
  "peerDependencies": {
    "deku": ">=0.5.6 <2.0.0",
    "redux": "^3.0.4"
  }
}
